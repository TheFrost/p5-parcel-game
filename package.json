{
  "name": "p5-parcel-game",
  "version": "1.0.0",
  "main": "src/js/game.app.js",
  "author": "TheFrost <frost084@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rm -rf .cache/ && parcel src/index.html --global GameApp --no-source-maps -d public/",
    "build": "rm -rf build/ && parcel build src/index.html --global GameApp --no-source-maps -d build/ && cp -R public/resources build/resources",
    "start:chart": "rm -rf .cache/ && parcel src/index-chart.html --global GameChart --no-source-maps -d public/",
    "build:chart": "rm -rf build/ && parcel build src/index-chart.html --global GameChart --no-source-maps -d build/"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "chart.js": "^2.7.2",
    "p5": "^0.6.1"
  }
}
